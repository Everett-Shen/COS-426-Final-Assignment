{
    "$schema": "https://json.schemastore.org/package.json",
    "repository": "cos426finalproject-vite",
    "name": "battlefield-princeton-attack-of-the-raccoons",
    "type": "module",
    "version": "0.1.0",
    "title": "Battlefield Princeton: Attack of the Raccoons",
    "description": "A swarm of rabid raccoons have descended upon the Princeton campus. To make matters worse, there are oblivious Princeton students walking around everywhere, made unaware of their surroundings by their Dean's Date assignments. The only thing standing between them and utter oblivion is you, a sentient golf cart.",
    "keywords": [
        "Princeton",
        "graphics",
        "three.js",
        "COS 426",
        "raccoons",
        "princeton",
        "students",
        "zombie defense",
        "but this last entry CANNOT have a comma after it ->"
    ],
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "prettier --write ."
    },
    "dependencies": {
        "dat.gui": "^0.7.9",
        "three": "^0.158.0"
    },
    "devDependencies": {
        "@types/dat.gui": "^0.7.12",
        "@types/three": "^0.158.3",
        "prettier": "^3.1.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.6.1"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "semi": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always",
        "printWidth": 80
    }
}
